    "test:it": "cypress open",
    "cy:ci": "start-server-and-test start http://localhost:3000 integration-tests",
    "unit-tests": "jest",
    "integration-tests": "cypress run",
    "test:ci": "npm test -- --coverage --watchAll=false",
    "test:cvg": "npm run test:ci -- --watchAll",
